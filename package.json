{
  "name": "@zavx0z/context",
  "version": "1.1.0",
  "description": "Контекст для MetaFor",
  "author": "zavx0z",
  "keywords": [
    "metafor"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zavx0z/context.git"
  },
  "bugs": {
    "url": "https://github.com/zavx0z/context/issues"
  },
  "docs": "https://zavx0z.github.io/context",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "type": "module",
  "private": false,
  "devDependencies": {
    "@types/bun": "^1.2.21",
    "concat-md": "^0.5.1",
    "dts-bundle-generator": "^9.5.1",
    "typedoc": "^0.28.12",
    "typedoc-plugin-markdown": "^4.8.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "scripts": {
    "build": "bun run script/build.ts --dev",
    "docs": "typedoc",
    "rules": "typedoc --options markdown.typedoc.json && concat-md --decrease-title-levels docs-markdown > rules.md",
    "prepublishOnly": "bun run build",
    "publish:npm": "bun publish --access public"
  }
}
